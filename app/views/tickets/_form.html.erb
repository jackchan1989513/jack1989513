<%= form_for(ticket) do |f| %>
  <% if ticket.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(ticket.errors.count, "error") %> prohibited this ticket from being saved:</h2>

      <ul>
      <% ticket.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :TicketNo %>
    <%= f.text_field :TicketNo %>
  </div>

  <div class="field">
    <%= f.label :CompanyName %>
    <%= f.text_field :CompantName %>
  </div>

  <div class="field">
    <%= f.label :CustomerName %>
    <%= f.text_field :CustomerName %>
  </div>

  <div class="field">
    <%= f.label :handler %>
    <%= f.select :Hundler, [[' ',' '],['Abc','Abc'],['Abcd','Abcd'],['Admin','Admin'],['Jack','Jack']] %>

  </div>

  <div class="field">
    <%= f.label :Description %>
    <%= f.text_area :Description %>
  </div>

  <div class="field">
    <%= f.label :ResponseTime %>
    <%= f.number_field :ResponseTime %>
  </div>

  <div class="field">
    <%= f.label :Status %>
    <%= f.select :Status, [['Open','Open'],['Fixed','Fixed'],['Closed','Closed']] %>




  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
