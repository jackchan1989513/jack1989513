<p id="notice"><%= notice %></p>

<h1>Internal Informations</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @internal_informations.each do |internal_information| %>
      <tr>
        <td><%= internal_information.Title %></td>
        <td><%= link_to 'Show', internal_information %></td>
        <td><%= link_to 'Edit', edit_internal_information_path(internal_information) %></td>
        <td><%= link_to 'Destroy', internal_information, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Internal Information', new_internal_information_path %>
