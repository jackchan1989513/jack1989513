<p id="notice"><%= notice %></p>

<h1>Access Logs</h1>

<table>
  <thead>
    <tr>
      <th>Dateandcompany</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @access_logs.each do |access_log| %>
      <tr>
        <td><%= access_log.DateAndCompany %></td>
        <td><%= link_to 'Show', access_log %></td>
        <td><%= link_to 'Edit', edit_access_log_path(access_log) %></td>
        <td><%= link_to 'Destroy', access_log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Access Log', new_access_log_path %>
